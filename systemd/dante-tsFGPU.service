[Unit]
Description=Dante SOCKS5 proxy inside tsFGPU namespace
After=tailscaled-tsFGPU.service
Requires=tailscaled-tsFGPU.service

[Service]
Type=simple
ExecStart=/usr/sbin/ip netns exec tsFGPU /usr/sbin/danted -f /etc/dante-tsFGPU.conf -N 1
Restart=on-failure
RestartSec=1

[Install]
WantedBy=multi-user.target
